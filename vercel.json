{
  "version": 2,
  "buildCommand": "npm run build:frontend",
  "installCommand": "npm install && cd frontend && npm install",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
